{
    "description": "PHP SDK for NETZFABRIK API",
    "keywords": [
        "netzfabrik",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://netzfabrik.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "NETZFABRIK",
            "email": "api@netzfabrik.com",
            "homepage": "https://netzfabrik.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "NETZFABRIK\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "NETZFABRIK\\Test\\" : "test/" }
    }
}
